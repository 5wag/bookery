{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"detail": "Build the Bookery.csproj project using dotnet build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"--no-restore",
				"${workspaceFolder}/Bookery.sln",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher": "$msCompile",
			"group": {
                "kind": "build",
                "isDefault": true
            }
		}
	]
}